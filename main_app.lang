Main
{
	main(cstr[] argv, cstr[] envp) static :none
	{
		printany "argv.size = ", argv.size(), '\n';
		int idx;
		idx = 0;
		while (idx < argv.size())
		{
			printany "argv[", idx, "]=", argv[idx], '\n';
			idx = idx + 1;
		}
		idx = 0;
		while (idx < envp.size())
		{
			printany "envp[", idx, "]=", envp[idx], '\n';
			idx = idx + 1;
		}
	}
}